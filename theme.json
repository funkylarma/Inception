{
	"$schema": "https://schemas.wp.org/wp/6.2/theme.json",
	"customTemplates": [
		{
			"name": "blank",
			"postTypes": [
				"page",
				"post"
			],
			"title": "Blank"
		},
		{
			"name": "404",
			"postTypes": [
				"page"
			],
			"title": "404"
		}
	],
	"settings": {
		"appearanceTools": true,
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": true,
			"defaultGradients": true,
			"defaultPalette": true,
			"duotone": [],
			"gradients": [],
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#4f4d4d",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#c92fd0",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#36d02f",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#e4e4e4",
					"name": "Tertiary",
					"slug": "tertiary"
				}
			],
			"text": true
		},
		"layout": {
			"contentSize": "50rem",
			"wideSize": "75rem"
		},
		"position": {
			"fixed": true
		},
		"shadow": {
			"defaultPresets": true,
			"presets": []
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
				"increment": 1.5,
				"mediumStep": 1.5,
				"operator": "*",
				"steps": 7,
				"unit": "rem"
			},
			"spacingSizes": [],
			"units": [
				"px",
				"em",
				"rem",
				"vh",
				"vw",
				"%"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": false,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system,BlinkMacSystemFont,\"Segoe UI\",Roboto,Oxygen-Sans,Ubuntu,Cantarell,\"Helvetica Neue\",sans-serif",
					"name": "System Fonts",
					"slug": "system-fonts"
				},
				{
					"fontFamily": "Geneva, Tahoma, Verdana, sans-serif",
					"slug": "geneva-verdana"
				},
				{
					"fontFamily": "Cambria, Georgia, serif",
					"slug": "cambria-georgia"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Open Sans",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/open-sans_normal_300.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/open-sans_normal_500.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/open-sans_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/open-sans_italic_300.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/open-sans_italic_500.ttf"
							]
						},
						{
							"fontFamily": "Open Sans",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/open-sans_italic_700.ttf"
							]
						}
					],
					"fontFamily": "Open Sans",
					"slug": "open-sans"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Plus Jakarta Sans",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans_normal_300.ttf"
							]
						},
						{
							"fontFamily": "Plus Jakarta Sans",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans_normal_700.ttf"
							]
						},
						{
							"fontFamily": "Plus Jakarta Sans",
							"fontStyle": "normal",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans_normal_800.ttf"
							]
						},
						{
							"fontFamily": "Plus Jakarta Sans",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans_italic_300.ttf"
							]
						},
						{
							"fontFamily": "Plus Jakarta Sans",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans_italic_700.ttf"
							]
						},
						{
							"fontFamily": "Plus Jakarta Sans",
							"fontStyle": "italic",
							"fontWeight": "800",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans_italic_800.ttf"
							]
						}
					],
					"fontFamily": "Plus Jakarta Sans",
					"slug": "plus-jakarta-sans"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "0.8rem",
						"min": "0.64rem"
					},
					"name": "Extra Small",
					"size": "0.8rem",
					"slug": "x-small"
				},
				{
					"fluid": {
						"max": "1rem",
						"min": "0.8rem"
					},
					"name": "Small",
					"size": "1rem",
					"slug": "small"
				},
				{
					"fluid": {
						"max": "1.125rem",
						"min": "1rem"
					},
					"name": "Medium",
					"size": "1.25rem",
					"slug": "medium"
				},
				{
					"fluid": {
						"max": "1.563rem",
						"min": "1.125rem"
					},
					"name": "Large",
					"size": "1.563rem",
					"slug": "large"
				},
				{
					"fluid": {
						"max": "1.953rem",
						"min": "1.563rem"
					},
					"name": "Extra Large",
					"size": "1.953rem",
					"slug": "x-large"
				},
				{
					"fluid": {
						"max": "2.441rem",
						"min": "1.953rem"
					},
					"name": "Extra Extra Large",
					"size": "2.441rem",
					"slug": "xx-large"
				},
				{
					"fluid": {
						"max": "3.052rem",
						"min": "2.441rem"
					},
					"name": "Hero",
					"size": "3.052rem",
					"slug": "hero"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/buttons": {
				"spacing": {
					"blockGap": "1rem"
				}
			},
			"core/calendar": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/code": {
				"border": {
					"radius": "0",
					"width": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"bottom": "25px",
						"left": "30px",
						"right": "30px",
						"top": "25px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/columns": {
				"spacing": {
					"blockGap": "50px",
					"padding": {
						"bottom": "50px"
					}
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comments-title": {
				"spacing": {
					"margin": {
						"bottom": "30px",
						"top": "60px"
					}
				}
			},
			"core/cover": {
				"spacing": {
					"padding": {
						"bottom": "30px",
						"left": "30px",
						"right": "30px",
						"top": "30px"
					}
				}
			},
			"core/heading": {
				"spacing": {
					"margin": {
						"bottom": "10px",
						"top": "0px"
					}
				},
				"typography": {
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--heading)"
				}
			},
			"core/image": {
				"spacing": {
					"margin": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "30px",
						"top": "0px"
					}
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)",
					"fontWeight": "var(--wp--custom--font-weight--regular)"
				}
			},
			"core/preformatted": {
				"border": {
					"color": "var(--wp--preset--color--contrast)",
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"bottom": "25px",
						"left": "30px",
						"right": "30px",
						"top": "25px"
					}
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--monospace)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/pullquote": {
				"spacing": {
					"padding": {
						"bottom": "30px",
						"left": "70px",
						"right": "30px",
						"top": "30px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/quote": {
				"color": {
					"text": "var:preset|color|secondary"
				},
				"spacing": {
					"margin": {
						"bottom": "35px"
					},
					"padding": {
						"bottom": "30px",
						"left": "70px",
						"right": "40px",
						"top": "30px"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|medium",
					"fontStyle": "normal",
					"fontWeight": "600",
					"lineHeight": "var(--wp--custom--line-height--medium)"
				}
			},
			"core/separator": {
				"border": {
					"radius": "0",
					"width": "0"
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"core/site-tagline": {
				"spacing": {
					"margin": {
						"bottom": "20px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--large)",
					"fontWeight": "var(--wp--custom--font-weight--regular)",
					"lineHeight": "var(--wp--custom--line-height--one)"
				}
			},
			"core/social-links": {
				"spacing": {
					"blockGap": "10px"
				}
			},
			"core/spacer": {
				"spacing": {
					"margin": {
						"top": "0 !important"
					}
				}
			},
			"core/table": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/template-part": {
				"spacing": {
					"margin": {
						"top": "0px !important"
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--contrast)",
					"text": "var(--wp--preset--color--base)"
				},
				"spacing": {
					"padding": {
						"bottom": "10px",
						"left": "25px",
						"right": "25px",
						"top": "10px"
					}
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "var(--wp--custom--font-weight--regular)"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"spacing": {
					"margin": {
						"bottom": "0px",
						"top": "10px"
					}
				},
				"typography": {
					"fontSize": "var:preset|font-size|x-small",
					"lineHeight": 1.3999999999999999
				}
			},
			"cite": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-small)",
					"fontStyle": "inherit"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-fonts)",
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-fonts)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-fonts)",
					"fontSize": "var(--wp--preset--font-size--large)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-fonts)",
					"fontSize": "var(--wp--preset--font-size--medium)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-fonts)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--system-fonts)",
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "1rem",
			"padding": {
				"bottom": "0px",
				"left": "1rem",
				"right": "1rem",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"lineHeight": "1.8"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		},
		{
			"area": "uncategorized",
			"name": "post-meta",
			"title": "Post Meta"
		}
	],
	"version": 2
}