{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#4f4d4d",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#003547",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#005E54",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#C2BB00",
					"name": "Tertiary",
					"slug": "tertiary"
				},
				{
					"color": "#E1523D",
					"name": "Quaternary",
					"slug": "quaternary"
				},
				{
					"color": "#ED8B16",
					"name": "Quinary",
					"slug": "quinary"
				},
				{
					"color": "#f5f5f5",
					"name": "Background",
					"slug": "background"
				}
			],
			"text": true
		},
		"layout": {
			"contentSize": "768px",
			"wideSize": "1080px"
		},
		"spacing": {
			"customSpacingSize": true,
			"spacingScale": {
				"increment": 1.5,
				"mediumStep": 1.5,
				"operator": "*",
				"steps": 0,
				"unit": "rem"
			},
			"spacingSizes": [
				{
					"name": "1",
					"size": "clamp(1.5rem, 5vw, 2rem)",
					"slug": "30"
				},
				{
					"name": "2",
					"size": "clamp(1.8rem, 1.8rem + ((1vw - 0.48rem) * 2.885), 3rem)",
					"slug": "40"
				},
				{
					"name": "3",
					"size": "clamp(2.5rem, 8vw, 4.5rem)",
					"slug": "50"
				},
				{
					"name": "4",
					"size": "clamp(3.75rem, 10vw, 7rem)",
					"slug": "60"
				},
				{
					"name": "5",
					"size": "clamp(5rem, 5.25rem + ((1vw - 0.48rem) * 9.096), 8rem)",
					"slug": "70"
				},
				{
					"name": "6",
					"size": "clamp(7rem, 14vw, 11rem)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "sans-serif",
					"name": "Sans Serif",
					"slug": "sans-serif"
				},
				{
					"fontFamily": "serif",
					"name": "Serif",
					"slug": "serif"
				},
				{
					"fontFace": [
						{
							"fontDisplay": "swap",
							"fontFamily": "Plus Jakarta Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans-v8-latin-200.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "Plus Jakarta Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "200",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans-v8-latin-200italic.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "Plus Jakarta Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans-v8-latin-regular.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "Plus Jakarta Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans-v8-latin-italic.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "Plus Jakarta Sans",
							"fontStretch": "normal",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans-v8-latin-700.woff2"
							]
						},
						{
							"fontDisplay": "swap",
							"fontFamily": "Plus Jakarta Sans",
							"fontStretch": "normal",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/plus-jakarta-sans-v8-latin-700italic.woff2"
							]
						}
					],
					"fontFamily": "\"Plus Jakarta Sans\", sans-serif",
					"name": "Plus Jakarta Sans",
					"slug": "plus-jakarta-sans"
				}
			],
			"fontSizes": [
				{
					"name": "Hero",
					"size": "4.0689rem",
					"slug": "hero"
				},
				{
					"name": "XX Large",
					"size": "3.2551rem",
					"slug": "xx-large"
				},
				{
					"name": "Extra Large",
					"size": "2.604rem",
					"slug": "extra-large"
				},
				{
					"name": "Large",
					"size": "2.0836rem",
					"slug": "large"
				},
				{
					"name": "Medium",
					"size": "1.6667rem",
					"slug": "medium"
				},
				{
					"name": "Normal",
					"size": "1.3333rem",
					"slug": "normal"
				},
				{
					"name": "Small",
					"size": "1.0667rem",
					"slug": "small"
				},
				{
					"name": "Extra Small",
					"size": "0.8533rem",
					"slug": "extra-small"
				},
				{
					"name": "XX Small",
					"size": "0.6827rem",
					"slug": "xx-small"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"border": {
					"radius": "100px"
				},
				"variations": {
					"outline": {
						"color": {
							"background": "var(--wp--preset--color--tertiary)",
							"text": "var(--wp--preset--color--contrast)"
						}
					}
				}
			},
			"core/cover": {
				"border": {
					"bottom": {
						"color": "var(--wp--preset--color--tertiary)",
						"style": "solid",
						"width": "1px"
					},
					"left": {
						"color": "var(--wp--preset--color--tertiary)",
						"style": "solid",
						"width": "1px"
					},
					"right": {
						"color": "var(--wp--preset--color--tertiary)",
						"style": "solid",
						"width": "1px"
					},
					"top": {
						"color": "var(--wp--preset--color--tertiary)",
						"style": "solid",
						"width": "1px"
					}
				}
			},
			"core/gallery": {
				"border": {
					"bottom": {
						"color": "var(--wp--preset--color--tertiary)",
						"style": "solid",
						"width": "1px"
					},
					"left": {
						"color": "var(--wp--preset--color--tertiary)",
						"style": "solid",
						"width": "1px"
					},
					"right": {
						"color": "var(--wp--preset--color--tertiary)",
						"style": "solid",
						"width": "1px"
					},
					"top": {
						"color": "var(--wp--preset--color--tertiary)",
						"style": "solid",
						"width": "1px"
					}
				},
				"color": {
					"background": "var(--wp--preset--color--base)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/paragraph": {
				"typography": {
					"fontFamily": "var(--var--preset--font-family--sans-serif)",
					"lineHeight": "1.7"
				}
			},
			"core/site-title": {
				"typography": {
					"fontStyle": "normal",
					"fontWeight": "300",
					"textDecoration": "none"
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--secondary)",
					"text": "var(--wp--preset--color--background)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--plus-jakarta-sans)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--quaternary)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
			"padding": {
				"bottom": "0",
				"left": "0",
				"right": "0",
				"top": "0"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--sans-serif)",
			"fontSize": "var(--wp--preset--font-size--small)",
			"fontStyle": "normal",
			"fontWeight": "300",
			"lineHeight": "1.7"
		}
	},
	"version": 2
}